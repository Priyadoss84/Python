from os import *
from sys import *
from collections import *
from math import *
import math

def squareRoot(a):

    # write your code here
    if a==1:
        return(1)
    elif a==0:
        return(0)
    else:
        y=1
        b=math.ceil(a/2)
        for i in range(1,b):
            if ((i*i <= a) and (i*i >=y)):
                y=i
        return (y)
    pass
